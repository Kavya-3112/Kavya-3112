#include<iostream>
#include<string>
using namespace std;
int main()
{
    string c;
    cin>>c;
    int cnt=0;
    for(int i=0;i<=c.size();i++)
    {
        if(c[i]==c[i+1])
        {
            cnt++;
        }
        else
        {
            cnt=0;
        }
        if(cnt>=6)
        {
            break;
        }
    
       
    }
    if(cnt>=6)
    {
        cout<<"YES";
    }
    else
    {
        cout<<"NO";
    }

}
